 174:			;
 175:			; Some commodity equs
 176:			;
 177:	000D'         	CR		EQU	0DH		; ascii CR & LF
 178:	000A'         	LF		EQU	0AH
 179:	000C'         	FF		EQU	0CH		; FORM FEED (clear screen)
 180:	001B'         	ESC		EQU	1BH		; ESCape
 181:	0011'         	XONC		EQU	11H		; Xon
 182:	0013'         	XOFC		EQU	13H		; Xoff
