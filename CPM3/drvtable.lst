  16:			dph 	macro 	?trans,?dpb,?csize,?asize
  17:				local ?csv,?alv
  18:				dw	?trans			; translate table address
  19:				db	0,0,0,0,0,0,0,0,0	; BDOS Scratch area
  20:				db	0			; media flag
  21:				dw	?dpb			; disk parameter block
  22:				if ?csize gt 0
  23:				dw	?csv			; checksum vector
  24:				else
  25:				dw	0fffeh			; checksum vector allocated by
  26:				endif				; GENCPM
  27:				if ?asize gt 0
  28:				dw	?alv			; allocation vector
  29:				else
  30:				dw	0fffeh			; alloc vector allocated by GENCPM
  31:				endif
  32:				dw	0fffeh			; dirbcb, dtabcb, hash alloc'd
  33:				dw	0fffeh			; by GENCPM
  34:				dw	0fffeh			;
  35:				db	0			; hash bank
  36:				if ?csize gt 0
  37:			?csv	ds	?csize			; checksum vector
  38:				endif
  39:				if ?asize gt 0
  40:			?alv	ds	?asize			; allocation vector
  41:				endif
  42:				endm
