 578:			;;
 579:			;; FDRVSEL - select drive for r/w ops
 580:			;
 581:			; FDRVSEL:
 582:			; 	PUSH	AF			;
 583:			; 	PUSH	HL			;
 584:			; 	LD	HL,HDRVV		; 10
 585:			; 	LD	A,(FDRVBUF)		; 13
 586:			; 	ADD	A,L			; 4
 587:			; 	LD	L,A			; 4
 588:			; 	LD	A,(HL)			; 7
 589:			; 	LD	H,A			;
 590:			; 	LD	A, (DSELBF)
 591:			; 	AND	$F0
 592:			; 	OR	H
 593:			; 	LD	(DSELBF),A		; 13
 594:			; 	OUT	(FDCDRVRCNT),A		; 11
 595:			; 	POP	HL			;
 596:			; 	POP	AF			;
 597:			; 	RET				;
 598:			; 	;
 599:			; 	; This used to translate the drive number in a cmd byte suitable
 600:			; 	; for drive selection on the floppy board
 601:			; HDRVV:	DEFB	$01			; drive 1
 602:			; 	DEFB	$02			; drive 2
 603:			; 	DEFB	$04			; drive 3
 604:			; 	DEFB	$08 			; drive 4
 605:			
 606:	0138          	fdrvsel:
 607:	0138  F5      		push	af			; save regs
 608:	0139  C5      		push	bc
 609:				
 610:	013A  3A4500  		ld	a,(fdrvbuf)		; load drive #
 611:	013D  47      		ld	b,a
 612:	013E  04      		inc 	b			; on b (+1 for loop)
 613:	013F  AF      		xor	a
 614:	0140  37      		scf
 615:	0141          	fdrvs0:
 616:	0141  17      		rla				; rotate to get
 617:	0142  10FD    		djnz	fdrvs0			; drive id
 618:				
 619:	0144  47      		ld	b,a			; save on b
 620:	0145  3A4E00  		ld	a,(dselbf)		; current select
 621:	0148  E6F0    		and	11110000b		; reset current id
 622:	014A  B0      		or	b			; and replace with new
 623:	014B  324E00  		ld	(dselbf),a		; update current
 624:	014E  D3D6    		out	(fdcdrvrcnt),a		; activate selection
 625:				
 626:	0150  C1      		pop	bc
 627:	0151  F1      		pop	af
 628:	0152  C9      		ret
