 713:	01A6  CB47    		bit	0,a			; test busy bit
 714:	01A8  C8      		ret	z			; exit if no command is in progress
 715:			; 	jr	z,fwait02		; jump if no command is in progress
 716:	01A9  1B      		dec	de			;
 717:	01AA  7A      		ld	a,d			; timer down
