;
; test receive 1
;

INIT	equ	$1
READY	equ	$5
ACK	equ	$7
OKGO	equ	$3

DATAP	equ	$3
CNTRP	equ	$2
BEEPP	equ	$8f
BUFFER	equ	$7000
PRINT	equ	$0f113
RAM3BUF	equ	$3043
CLEAR	equ	$0f0e7
DISPSTR equ	$0f2e1
STRING	equ	$0f301
LOCATE	equ	$0f06e

	org $100

START:	
	;call CLEAR
	ld hl,$0000
	call LOCATE
	ld b, $0ff
LOOP:
	ld a, b
	call PRINT
	djnz LOOP
	ret

	org $200
STRING:
	defb 0,0
	defs "CON DISPTR"
	defb 0
	
	end